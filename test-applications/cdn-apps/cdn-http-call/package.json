{
  "name": "cdn_http_call",
  "version": "0.0.1",
  "description": "FastEdge CDN application for integration tests - proxy_http_call dispatch",
  "type": "module",
  "scripts": {
    "build": "npm-run-all -s clean build:all copy:all clean",
    "build:all": "npm-run-all -p build:http-call build:all-hooks-http-call",
    "build:http-call": "asc assembly/http-call.ts --target release --outFile build/http-call.wasm --textFile build/http-call.wat --sourceMap --optimize",
    "build:all-hooks-http-call": "asc assembly/all-hooks-http-call.ts --target release --outFile build/all-hooks-http-call.wasm --textFile build/all-hooks-http-call.wat --sourceMap --optimize",
    "copy:all": "npm-run-all -p copy:http-call copy:all-hooks-http-call",
    "copy:http-call": "npx cross-env NAME=http-call npm run mv:file",
    "copy:all-hooks-http-call": "npx cross-env NAME=all-hooks-http-call npm run mv:file",
    "mv:file": "mv build/$NAME.wasm ../../../wasm/cdn-apps/http-call/$NAME.wasm",
    "clean": "rm -rf build && mkdir -p build"
  },
  "dependencies": {},
  "devDependencies": {},
  "license": "ISC"
}
