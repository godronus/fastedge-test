                "headers",
                "body"
              ]
            },
            "properties": {
              "type": "object",
              "additionalProperties": {}
            }
          },
          "required": [
            "request",
            "response"
          ]
        },
        "properties": {
          "type": "object",
          "additionalProperties": {}
        }
